 "scripts": {
        "test": "export NODE_TLS_REJECT_UNAUTHORIZED=0; mocha --timeout 15000 -r ts-node/register 'test/src/**/*.ts'",
        "test:linter-only": "export NODE_TLS_REJECT_UNAUTHORIZED=0; mocha --timeout 15000 -r ts-node/register 'test/src/linter/**/*.ts'",
        "test:coverage": "nyc --reporter=lcov --reporter=text npm run test",
        "lint": "gts lint",
        "clean": "gts clean",
        "compile": "tsc",
        "fix": "gts fix",
        "prepare": "npm run compile && husky install .githooks",
        "pretest": "npm run compile",
        "posttest": "npm run lint",
        "start": "npm run prepare && node build/index.js",
        "import-agent": "ts-node src/index.ts -i",
        "import-agent-with-bucket": "ts-node src/index.ts --import --with-bucket",
        "export-agent": "ts-node src/index.ts -e",
        "export-agent-blob": "ts-node src/index.ts --export blob",
        "export-agent-json": "ts-node src/index.ts -e json",
        "export-agent-json-with-ids": "npm run export-agent-json -- --keep-ids",
        "validate-agent-json": "ts-node src/index.ts -v",
        "export-with-transitions-file": "ts-node src/index.ts -e json --keep-ids -t",
        "generate-transitions-file": "ts-node src/index.ts -t",
        "generate-bilingual-transitions-file": "ts-node src/index --bilingual-transitions"
    },
